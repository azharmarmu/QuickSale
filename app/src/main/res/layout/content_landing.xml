<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="azhar.com.quicksale.activity.LandingActivity"
    tools:showIn="@layout/app_bar_landing">

    <include
        android:id="@+id/order_holder"
        layout="@layout/view_order"
        android:visibility="gone" />

    <include
        android:id="@+id/taken_holder"
        layout="@layout/view_taken"
        android:visibility="gone" />

    <include
        android:id="@+id/sales_holder"
        layout="@layout/view_sales"
        android:visibility="gone" />

    <include
        android:id="@+id/return_holder"
        layout="@layout/view_return"
        android:visibility="gone" />

    <!--Resources-->

    <include
        android:id="@+id/customer_holder"
        layout="@layout/view_customer"
        android:visibility="gone" />

    <include
        android:id="@+id/sales_man_holder"
        layout="@layout/view_sales_man"
        android:visibility="gone" />

    <include
        android:id="@+id/setup_holder"
        layout="@layout/view_setup"
        android:visibility="gone" />

</android.support.constraint.ConstraintLayout>
